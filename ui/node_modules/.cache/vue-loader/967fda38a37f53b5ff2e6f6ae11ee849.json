       {\n          icon: \"dashboard\",\n          title: \"Dashboard\",\n          path: \"/\"\n        },\n        {\n          icon: \"devices\",\n          title: \"Devices\",\n          path: \"/devices\"\n        },\n        {\n          icon: \"history\",\n          title: \"Sessions\",\n          path: \"/sessions\"\n        }\n      ]\n    };\n  },\n\n  computed: {\n    tenant() {\n      return this.$store.getters[\"auth/tenant\"];\n    },\n\n    isLoggedIn() {\n      return this.$store.getters[\"auth/isLoggedIn\"];\n    }\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch(\"auth/logout\").then(() => {\n        this.$router.push(\"/login\");\n      });\n    }\n  }\n};\n</script>\n\n<style>\n.v-list-active {\n  border-left: 4px solid var(--v-primary-base);\n}\n\n.text-shadow {\n  text-shadow: #000 0 0 6px;\n  color: transparent;\n}\n</style>\n"]}]}