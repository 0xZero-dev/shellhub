AzBA;AAdA,KAAA;AA+CA,GAxDA;AA0DA,EAAA,KAAA,EAAA;AACA,IAAA,UAAA,EAAA;AACA,MAAA,OADA,qBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,WADA,GACA,KAAA,CAAA,UAAA,CAAA,YADA;AAEA,kBAAA,UAFA,GAEA,KAAA,CAAA,UAAA,CAAA,IAFA;AAIA,kBAAA,KAAA,CAAA,IAAA,GAAA,CAAA;AAAA,gCAAA,WAAA;AAAA,4BAAA;AAAA,mBAAA,CAAA;AAJA;AAAA,yBAMA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,eAAA,EAAA,KAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CANA;;AAAA;AAOA,kBAAA,KAAA,CAAA,WAAA,GAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,kBAAA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,0BAAA,CAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,OAVA;AAWA,MAAA,IAAA,EAAA;AAXA;AADA,GA1DA;AA0EA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,KADA,EACA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,aAAA,KAAA,CAAA,GAAA;AACA,KAHA;AAKA,IAAA,OALA,mBAKA,IALA,EAKA;AACA,uBAAA,IAAA,CAAA,SAAA,cAAA,IAAA,CAAA,IAAA,cAAA,KAAA,QAAA;AACA,KAPA;AASA,IAAA,IATA,gBASA,MATA,EASA;AACA,WAAA,UAAA,CAAA,MAAA,CAAA,GAAA;AACA,KAXA;AAaA,IAAA,MAbA,kBAaA,GAbA,EAaA;AACA,UAAA,OAAA,CAAA,eAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,GAAA;AACA;AACA,KAjBA;AAmBA,IAAA,eAnBA,2BAmBA,IAnBA,EAmBA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,yBAAA,EAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,YAAA;AACA,KAvBA;AAyBA,IAAA,cAzBA,0BAyBA,IAzBA,EAyBA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,wBAAA,EAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,YAAA;AACA,KA7BA;AA+BA,IAAA,aA/BA,2BA+BA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAjCA;AAmCA,IAAA,IAnCA,gBAmCA,IAnCA,EAmCA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA;AACA,QAAA,GAAA,EAAA,IAAA,CAAA,GADA;AAEA,QAAA,IAAA,EAAA,KAAA;AAFA,OAAA;AAKA,MAAA,IAAA,CAAA,IAAA,GAAA,KAAA,QAAA;AACA;AA1CA;AA1EA,CAAA","sourcesContent":["<template>\n<fragment>\n\n  <div class=\"d-flex pa-0 align-center\">\n    <h1>Devices</h1>\n    <v-spacer/>\n    <v-spacer/>\n    <DeviceAdd/>\n    <v-btn outlined @click=\"$store.dispatch('modals/showAddDevice', true)\">Add Device</v-btn>\n  </div>\n    \n  <v-card class=\"mt-2\">\n    <v-app-bar flat color=\"transparent\"></v-app-bar>\n    <v-divider></v-divider>\n\n    <v-card-text class=\"pa-0\">\n      <v-data-table :headers=\"headers\" :items=listDevices item-key=\"uid\" :sort-by=\"['started_at']\" :sort-desc=\"[true]\" :items-per-page=\"10\" :footer-props=\"{'items-per-page-options': [10, 25, 50, 100]}\" :server-items-length='this.numberDevices' :options.sync='pagination' :disable-sort=\"true\" >\n\n        <template v-slot:item.online=\"{ item }\" >\n          <v-icon color=\"success\" v-if=\"item.online\" @click.stop=\"detailsDevice(item)\">check_circle</v-icon>\n          <v-tooltip bottom v-else>\n            <template #activator=\"{ on }\">\n              <v-icon v-on=\"on\">check_circle</v-icon>\n            </template>\n            <span>last seen {{ item.last_seen | moment(\"from\", \"now\") }}</span>\n          </v-tooltip>\n        </template>\n\n        <template v-slot:item.uid=\"{ item }\">\n          <v-chip class=\"short\" color=\"blue lighten-2\">\n            <span>{{ item.uid }}</span>\n            <v-icon small dark right@click.stop v-clipboard=\"item.uid\" v-clipboard:success=\"showCopySnack\">mdi-content-copy none-text</v-icon>\n          </v-chip>\n        </template>\n\n        <template v-slot:item.info.pretty_name=\"{ item }\">\n          <v-icon left>{{ deviceIcon[item.info.id] || 'fl-tux' }}</v-icon>\n          {{ item.info.pretty_name }}\n        </template>\n\n        <template v-slot:item.identity.mac=\"{ item }\">\n          <code>{{ item.identity.mac }}</code>\n        </template>\n\n        <template v-slot:item.namespace=\"{ item }\">\n          <v-chip class=\"list-itens\">\n            {{ address(item) }}<v-icon small right @click.stop v-clipboard=\"() => address(item)\" v-clipboard:success=\"showCopySnack\">mdi-content-copy</v-icon>\n          </v-chip>\n        </template>\n        \n        <template v-slot:item.call_home=\"{ item }\">\n           <v-icon class=\"icons\" @click=\"disableCallhome(item)\">cancel</v-icon>\n           <v-icon class=\"icons\" @click=\"enableCallhome(item)\">check_circle</v-icon>\n        </template>\n\n        <template v-slot:item.actions=\"{ item }\">\n          <v-icon class=\"icons\" @click=\"detailsDevice(item)\">info</v-icon>\n\n          <TerminalDialog :uid=\"item.uid\" v-if=\"item.online\"></TerminalDialog>\n\n          <v-icon @click=\"remove(item.uid)\">delete</v-icon>\n        </template>\n\n      </v-data-table>\n    </v-card-text>\n    <v-snackbar v-model=\"copySnack\" :timeout=3000>Device SSHID copied to clipboard</v-snackbar>\n    <v-snackbar v-model=\"showMsg\" :timeout=3000>Device call home status changed. Page is going to refresh</v-snackbar>\n  </v-card>\n\n</fragment>\n</template>\n\n<script>\nimport TerminalDialog from '@/components/terminal/TerminalDialog.vue';\nimport DeviceAdd from '@/components/device/DeviceAdd.vue';\n\nexport default {\n  name: 'DeviceList',\n\n  components: {\n    TerminalDialog,\n    DeviceAdd\n  },\n\n  data() {\n    return {\n      hostname: window.location.hostname,\n      numberDevices: 0,\n      listDevices: [],\n      pagination: {},\n      deviceIcon: {\n        arch: 'fl-archlinux',\n        ubuntu: 'fl-ubuntu',\n        raspbian: 'fl-raspberry-pi',\n        'ubuntu-core': 'fl-ubuntu'\n      },\n      copySnack: false,\n      showMsg: false,\n      editName: '',\n      headers: [\n        {\n          text: 'Online',\n          value: 'online',\n          align: 'center'\n        },\n        {\n          text: 'Name',\n          value: 'name'\n        },\n        {\n          text: 'Operating System',\n          value: 'info.pretty_name'\n        },\n\n        {\n          text: 'SSHID',\n          value: 'namespace',\n          align: 'center'\n        },\n        {\n          text: 'Call Home',\n          value: 'call_home',\n          align: 'center'\n        },\n        {\n          text: 'Actions',\n          value: 'actions',\n          align: 'center',\n          sortable: false\n        }\n      ]\n    };\n  },\n\n  watch: {\n    pagination: {\n      async handler () {\n        const rowsPerPage = this.pagination.itemsPerPage\n        const pageNumber = this.pagination.page\n\n        this.data =  [{'per_page': rowsPerPage, 'page':pageNumber}]\n\n        await this.$store.dispatch(\"devices/fetch\", this.data[0]);\n        this.listDevices = this.$store.getters[\"devices/list\"];\n        this.numberDevices = this.$store.getters[\"devices/getNumberDevices\"]; \n      },\n      deep: true\n    },\n  },\n\n  methods: {\n    detailsDevice(value){\n      this.$router.push('/device/'+value.uid) \n    },\n\n    address(item) {\n      return `${item.namespace}.${item.name}@${this.hostname}`;\n    },\n\n    copy(device) {\n      this.$clipboard(device.uid);\n    },\n\n    remove(uid) {\n      if (confirm('Are you sure?')) {\n        this.$store.dispatch('devices/remove', uid);\n      }\n    },\n\n    disableCallhome(item) {\n        this.$store.dispatch('devices/disableCallhome', item);\n      this.showMsg = true;\n       this.$forceUpdate();\n    },\n    \n    enableCallhome(item) {\n        this.$store.dispatch('devices/enableCallhome', item);\n      this.showMsg = true;\n       this.$forceUpdate();\n    },\n\n    showCopySnack() {\n      this.copySnack = true;\n    },\n\n    save(item) {\n      this.$store.dispatch('devices/rename', {\n        uid: item.uid,\n        name: this.editName\n      });\n\n      item.name = this.editName;\n    }\n  },\n  \n};\n</script>\n<style scoped>\n.list-itens {\n  font-family: monospace;\n}\n\n.icons{\n  margin-right: 4px;\n}\n\n</style>\n"],"sourceRoot":"src/components/device"}]}